package com.mukesh.microservices.limitsservice.bean;

public class LimitsConfiguration {
	
	int maximum;
	int minium;
	public int getMaximum() {
		return maximum;
	}
	public void setMaximum(int maximum) {
		this.maximum = maximum;
	}
	public int getMinium() {
		return minium;
	}
	
	public LimitsConfiguration() {
		super();
	}
	public LimitsConfiguration(int maximum, int minium) {
		super();
		this.maximum = maximum;
		this.minium = minium;
	}
	public void setMinium(int minium) {
		this.minium = minium;
	}
	
}
